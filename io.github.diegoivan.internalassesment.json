{
    "app-id" : "io.github.diegoivan.internalassesment",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "master",
    "sdk" : "org.gnome.Sdk",
    "command" : "internal-assesment",
    "sdk-extensions" : [
    "org.freedesktop.Sdk.Extension.vala"
  ],
  "build-options" : {
    "prepend-path" : "/usr/lib/sdk/vala/bin/",
    "prepend-ld-library-path" : "/usr/lib/sdk/vala/lib"
  },
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "/share/vala",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "internal-assesment",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "."
                }
            ]
        }
    ]
}
